{
  "name": "aws-secrets-extension",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20.x"
  },
  "packageManager": "yarn@4.4.1",
  "scripts": {
    "postinstall": "husky install",
    "build": "tsc",
    "build-local-extension": "docker build -f local/Dockerfile.local-extension -t local-extension .",
    "build-lambda-environment": "docker build -f local/Dockerfile.lambda-environment -t lambda-environment .",
    "run-local-extension": "docker compose --file local/compose.yml up",
    "start-local": "npm-run-all build-local-extension build-lambda-environment run-local-extension",
    "test": "vitest run unit --coverage",
    "test-watch": "vitest unit --coverage",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "prepare": "husky"
  },
  "imports": {
    "#lib/*": "./lib/*"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/koa": "^2",
    "@types/koa__router": "^12.0.4",
    "@types/node": "20.16.10",
    "@vitest/coverage-istanbul": "^2.1.2",
    "aws-cdk-lib": "2.161.0",
    "constructs": "^10.3.0",
    "eslint": "^9.11.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "npm-run-all": "^4.1.5",
    "rambda": "^9.3.0",
    "tsx": "^4.19.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.156.0",
    "constructs": "^10.0.0"
  },
  "files": [
    "dist/*",
    "package.json",
    "README.MD"
  ],
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.664.0",
    "@aws-sdk/client-ssm": "^3.664.0",
    "@koa/router": "^13.0.0",
    "esbuild": "^0.23.1",
    "koa": "^2.15.3",
    "zod": "^3.23.8"
  }
}
